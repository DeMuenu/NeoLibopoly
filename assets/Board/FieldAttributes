from player import playerclass

class templateField:
    def __init__(self):
        self.cost = 10000
        self.rent = 500
        self.houseRentMultiplier = 1,5
        self.maxhouses = 3
        self.houses = 0
        self.owner = 0


    def buyHouse(self):
        if self.owner != 0:
            if self.houses != self.maxhouses:
                self.houses = self.houses + 1
                self.rent = self.rent * self.houseRentMultiplier

    
    def landOnField(self, landedPlayer):
        if self.owner != 0:
            if self.owner != landedPlayer:
                landedPlayer.money
 